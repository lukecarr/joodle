{
  "name": "joodle",
  "version": "0.2.3",
  "description": "Moodle Web Service API client for Node.js.",
  "keywords": [
    "moodle",
    "moodle-ws",
    "moodle-api"
  ],
  "homepage": "https://joodle.lukecarr.dev",
  "bugs": {
    "url": "https://github.com/lukecarr/joodle/issues"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Luke Carr",
      "email": "me@lukecarr.dev"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukecarr/joodle.git"
  },
  "scripts": {
    "docs:jsdoc": "vuepress-jsdoc --source ./src --dist ./docs --folder reference --title \"API Reference\" -c ./jsdoc.json",
    "docs:dev": "vuepress dev docs",
    "docs:build": "yarn docs:jsdoc && vuepress build docs",
    "lint:eslint": "eslint \"src/**\" \"tests/**\"",
    "lint:prettier": "prettier *.md **/*.md *.json **/*.yml",
    "lint:check": "yarn lint:eslint && yarn lint:prettier --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "test:unit": "jest --passWithNoTests",
    "test:coverage": "codecov",
    "test": "yarn test:unit && yarn test:coverage",
    "build:clean": "rimraf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "prepublishOnly": "yarn lint:check && yarn test && yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.11",
    "@types/qs": "^6.9.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "nock": "^13.0.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "vuepress": "^1.7.1",
    "vuepress-jsdoc": "^3.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "cacheable-request": "^7.0.1",
    "got": "^11.8.0",
    "qs": "^6.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
